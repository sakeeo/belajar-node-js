generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model absensi {
  idabsensi Int            @id @default(autoincrement())
  idpegawai Int
  tanggal   DateTime       @db.Date
  in        DateTime       @db.DateTime(0)
  out       DateTime       @db.DateTime(0)
  status    absensi_status @default(online)

  @@map("23_absensi")
}

model absensi_photo {
  idabsensiphoto Int                 @id @default(autoincrement())
  idabsensi      Int
  photo          String              @db.VarChar(255)
  jenis          absensi_photo_jenis
  waktu          DateTime            @db.DateTime(0)
  lokasi         String              @db.VarChar(255)
  latitude       String              @db.VarChar(100)
  longtitude     String              @db.VarChar(100)

  @@map("23_absensi_photo")
}

model customer_knowledge {
  id           Int                    @id @default(autoincrement())
  customer_id  String                 @db.VarChar(20)
  field        String                 @db.VarChar(255)
  val          String                 @db.VarChar(255)
  date_created DateTime               @default(now()) @db.Timestamp(0)
  date_updated DateTime               @default(now()) @db.Timestamp(0)
  del          customer_knowledge_del @default(n)

  @@map("23_customer_knowledge")
}

model menu_galley {
  id_menu    Int                    @id @default(autoincrement())
  nama_menu  String                 @db.VarChar(100)
  jenis_menu menu_galley_jenis_menu @default(main)
  link_url   String                 @db.VarChar(256)
  icon       String                 @db.VarChar(128)
  urutan     Int
  main_id    Int?

  @@map("23_menu_galley")
}

model opname {
  id         Int           @id @default(autoincrement())
  idoutlet   String        @db.VarChar(25)
  iduser     Int
  status     opname_status @default(pending)
  created_at DateTime      @default(now()) @db.Timestamp(0)
  updated_at DateTime      @default(now()) @db.Timestamp(0)

  @@map("23_opname")
}

model opname_item {
  id          Int                      @id @default(autoincrement())
  opname_id   Int
  idsku       String                   @db.VarChar(25)
  iduser      Int
  fisik       opname_item_fisik?
  pricetag    opname_item_pricetag?
  atc         opname_item_atc?
  note        String?                  @db.Text
  image       String?                  @db.VarChar(255)
  created_at  DateTime                 @default(now()) @db.Timestamp(0)
  updated_at  DateTime                 @default(now()) @db.Timestamp(0)
  status_item opname_item_status_item? @default(pending)

  @@map("23_opname_item")
}

model pegawai {
  idpegawai    Int         @id @default(autoincrement())
  namadepan    String      @db.VarChar(25)
  namabelakang String      @db.VarChar(25)
  hp           String      @db.VarChar(25)
  alamat       String      @db.VarChar(255)
  photo        String      @db.VarChar(255)
  date_created DateTime    @default(now()) @db.Timestamp(0)
  date_updated DateTime    @default(now()) @db.Timestamp(0)
  del          pegawai_del @default(n)

  @@map("23_pegawai")
}

model pegawai_outlet {
  idpegawaioutlet Int    @id @default(autoincrement())
  idpegawai       Int
  idoutlet        String @db.VarChar(10)

  @@map("23_pegawai_outlet")
}

model produk_kategori {
  id           Int      @id @default(autoincrement())
  kategori     String   @db.VarChar(255)
  date_created DateTime @default(now()) @db.Timestamp(0)
  date_updated DateTime @default(now()) @db.Timestamp(0)

  @@map("23_produk_kategori")
}

model produk_sub_kategori {
  id                 Int      @id @default(autoincrement())
  produk_kategori_id Int
  sub_kategori       String   @db.VarChar(255)
  date_created       DateTime @default(now()) @db.Timestamp(0)
  date_updated       DateTime @default(now()) @db.Timestamp(0)

  @@map("23_produk_sub_kategori")
}

model share_ba {
  id           Int                @id @default(autoincrement())
  kategori     share_ba_kategori?
  min          Float?
  persentase   Int?
  date_created DateTime?          @db.Timestamp(0)
  date_updated DateTime?          @db.Timestamp(0)

  @@map("23_share_ba")
}

model user_gallery_access {
  id                 Int @id @default(autoincrement())
  user_gallery_ba_id Int
  menu_id            Int

  @@map("23_user_gallery_access")
}

model user_gallery_ba_outlet {
  id                 Int    @id @default(autoincrement())
  user_gallery_ba_id Int
  idoutlet           String @db.VarChar(10)

  @@map("23_user_gallery_ba_outlet")
}

model users23 {
  iduser    Int    @id @default(autoincrement())
  uuid      String @db.VarChar(255)
  username  String @db.VarChar(150)
  password  String @db.VarChar(250)
  idpegawai Int
  level     Int

  @@map("23_users")
}

model costumeusers {
  id                BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  name              String    @db.VarChar(255)
  email             String    @unique(map: "users_email_unique") @db.VarChar(255)
  email_verified_at DateTime? @db.Timestamp(0)
  password          String    @db.VarChar(255)
  remember_token    String?   @db.VarChar(100)
  api_token         String?   @db.VarChar(255)
  created_at        DateTime? @default(now()) @db.Timestamp(0)
  updated_at        DateTime? @default(now()) @db.Timestamp(0)
}

model failed_jobs {
  id         BigInt   @id @default(autoincrement()) @db.UnsignedBigInt
  uuid       String   @unique(map: "failed_jobs_uuid_unique") @db.VarChar(255)
  connection String   @db.Text
  queue      String   @db.Text
  payload    String   @db.LongText
  exception  String   @db.LongText
  failed_at  DateTime @default(now()) @db.Timestamp(0)
}

model groups {
  id          Int    @id @default(autoincrement()) @db.UnsignedMediumInt
  name        String @db.VarChar(20)
  description String @db.VarChar(100)
}

model login_attempts {
  id         Int    @id @default(autoincrement()) @db.UnsignedInt
  ip_address String @db.VarChar(45)
  login      String @db.VarChar(100)
  time       Int?   @db.UnsignedInt
}

model penagihan_piutang {
  id               Int      @id @default(autoincrement())
  nonota           String   @db.VarChar(80)
  tanggalpenagihan DateTime @db.Date
  keterangan       String   @db.Text
}

model personal_access_tokens {
  id             BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  tokenable_type String    @db.VarChar(255)
  tokenable_id   BigInt    @db.UnsignedBigInt
  name           String    @db.VarChar(255)
  token          String    @unique(map: "personal_access_tokens_token_unique") @db.VarChar(64)
  abilities      String?   @db.Text
  last_used_at   DateTime? @db.Timestamp(0)
  created_at     DateTime? @default(now()) @db.Timestamp(0)
  updated_at     DateTime? @default(now()) @db.Timestamp(0)
  expires_at     DateTime  @default(now()) @db.Timestamp(0)

  @@index([tokenable_type, tokenable_id], map: "personal_access_tokens_tokenable_type_tokenable_id_index")
}

model tb_biaya_tambahan {
  id_biaya_tambahan String  @id @db.VarChar(25)
  nama_biaya        String? @db.VarChar(255)
}

model tb_box {
